def q(help):
    return input(help)
num = q('Enter a number: ')

try:
    fnum = float(num)
except:
    num = q('Enter a numerical number: ') 
    
inum = int(num)
    
a = [x for x in range(2,inum) if inum%x == 0]

def is_prime(n):
    if n>1:
        if len(a) == 0:
            print('%s is prime' %inum)
        else:
            print('%s is not prime' %inum)
    else:
        print('%s is not prime' %inum)



is_prime(inum)

